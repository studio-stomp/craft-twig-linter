{
    "name": "studio-stomp/craft-twig-linter",
    "description": "A module for Craft CMS bringing `sserbin/twig-linter` to Craft CMS projects",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-module",
        "craft twig linter"
    ],
    "support": {
        "docs": "https://github.com/studio-stomp/craft-twig-linter/blob/main/README.md",
        "issues": "https://github.com/studio-stomp/craft-twig-linter/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Studio Stomp",
            "homepage": "https://studiostomp.nl"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "craftcms/cms": "^3.5.0",
        "sserbin/twig-linter": "^2.1"
    },
    "require-dev": {
        "codeception/codeception": "^3.1",
        "roave/security-advisories": "dev-master",
        "studio-stomp/phpstan-craftcms": "^0.1.0",
        "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "studiostomp\\crafttwiglinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\modules\\": "tests/_craft/modules/"
        }
    }
}
